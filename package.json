{
  "version": "0.0.1",
  "name": "prismax",
  "repository": "git@github.com:prisma-x/prismax.git",
  "author": "Sajjad Shirazy <shirazy.sajjad@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon -e ts -w ./src -x npm start",
    "build": "tsc -d",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "release:patch": "standard-version && git push --tags",
    "release:minor": "standard-version --release-as minor && git push --tags",
    "release:major": "standard-version --release-as major && git push --tags"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "standard-version": "^7.1.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-preview022",
    "prisma2": "^2.0.0-preview022"
  }
}
